<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//FR"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 <hibernate-mapping>
	 <class name="bean.Contact" table="contacts">
	 	<id name="id" column="id">
	 		<generator class="increment"></generator>
	 	</id>
	<property name="name" column="name"></property>
	<!--  <property name="lastname" column="lastname"></property>-->
	<property name="email" column="email"></property>
	<property name="phone" column="phone"></property>
	<property name="subject" column="subject"></property>
	<property name="message" column="message"></property>
	<property name="created" column="created"></property>
</class>
</hibernate-mapping>
	 	